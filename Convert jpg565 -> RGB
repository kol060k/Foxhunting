uint16_t red_mask = 0xF800;
uint16_t green_mask = 0x7E0;
uint16_t blue_mask = 0x1F;

#define size_im 32

int new_im[size_im][size_im][3] = {0};

void write()
{
    int i, j, te;
	FILE * out  = fopen ("C:\\Users\\S\\Desktop\\Project\\out.h", "w");
    te = fwrite(" { // flag\n", 11, 1, out);
	for (i = 0; i < size_im; ++i)
        for (j = size_im - 1; j >= 0; --j)
            {
                fprintf(out, "%d", new_im[i][j][0]);
                te = fwrite (", ", 2, 1, out);
                fprintf(out, "%d", new_im[i][j][1]);
                te = fwrite (", ", 2, 1, out);
                fprintf(out, "%d", new_im[i][j][2]);
                te = fwrite (",\n", 2, 1, out);
            }
    te = fwrite("},", 2, 1, out);
	fclose (out);
}

int main()
{
    int i, j, te, k = size_im * size_im - 1;
	for (i = 0; i < size_im; ++i)
    {
        for (j = 0; j < size_im; ++j)
            {
                new_im[i][j][0] = ((im[k] & red_mask) >> 11) * 8;
                new_im[i][j][1] = ((im[k] & green_mask) >> 5) * 4;
                new_im[i][j][2] = ((im[k] & blue_mask)) * 8;
                --k;
            }
    }
    write();
    return 0;
}
